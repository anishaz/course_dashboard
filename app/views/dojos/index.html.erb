<h1>Dojo Dashboard</h1>

<h2>Listing <%= pluralize(@dojos.count, "Dojo") %></h2>

<table>
  <thead>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
  </thead>
  <tbody>
    <% @dojos.each do |dojo| %>
    <tr>
      <td><%= dojo.branch %></td>
      <td><%= dojo.street %></td>
      <td><%= dojo.city %></td>
      <td><%= dojo.state %></td>
    </tr>
    <% end %>
  </tbody>
</table>
