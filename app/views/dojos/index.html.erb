<h1>Dojo Dashboard</h1>

<h2>Listing <%= @count %> Dojos</h2>

<table>
  <thead>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Action</th>
  </thead>
  <tbody>
    <% @dojos.each do |dojo| %>
    <tr>
      <td><%= dojo.branch %></td>
      <td><%= dojo.street %></td>
      <td><%= dojo.city %></td>
      <td><%= dojo.state %></td>
      <td><a href='/dojos/<%= dojo.id %>'>Show</a>|<a href='/dojos/<%= dojo.id%>/edit'>Edit</a>|<a href="/dojos/<%= dojo.id %>" data-confirm="Are you sure?" data-method="delete">Delete</a>
    </tr>
    <% end %>
  </tbody>
</table>

<a href="/dojos/new">Create a new dojo</a>
